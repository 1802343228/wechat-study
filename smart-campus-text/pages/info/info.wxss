/* pages/info/info.wxss */
.clearfix:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
.positionFixed{
  position: fixed;
  left: 0;
  top: 0;
}

page{
  width: 100%;
  height:100%;
}
.layout{
  width: 100%;
  height: 100%;
  background: #eee;
}
.banner{
  width: 100%;
  height: 200px;
  line-height: 200px;
  background: #FFB11A ;
  
}
.banner text{
  text-align: center;
  display: block;
}
.nav{
  width: 100%;
  height: 45px;
  line-height: 45px;
  background: #fff;
}
.nav_row{
  float: left;
  width: 25%;
  font-family: PingFangSC-Light;
  font-size: 16px;
  color: #333333;
}
.nav_row text{
  text-align: center;
  display: block;
}
.content {
  width: 100%;
  height: 200px;
  font-family: PingFangSC-Light;
  font-size: 16px;
  color: #333333;
  padding: 15px;
}
.content1{
  background: #F5BBA4;
}
.content2{
  background: #E9ED9A;
}
.content3{
  background: #9DE59C;
}
.content4{
  background: #98A5E2;
}